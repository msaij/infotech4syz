{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": "src",
    "paths": {
      "@/*": ["./*"],
      "@/styles/*": ["styles/*"],
      "@/components/*": ["app/components/*"],
      "@/start/*": ["app/(authenticated)/start/*"],
      "@/clients/*": ["app/(authenticated)/clients/*"],
      "@/start-components/*": ["app/(authenticated)/start/components/*"],
      "@/client-components/*": ["app/(authenticated)/clients/components/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
